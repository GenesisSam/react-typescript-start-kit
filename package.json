{
  "name": "typescript-react-start-kit",
  "version": "1.0.0",
  "description": "This is useful start kit for Typescript, React, Sass, Redux, Material-ui, Webpack2, immutable",
  "main": "src/App.tsx",
  "scripts": {
    "build:prod": "npm install && rm -rf dist/ && ENV_NODE=production webpack -p --config webpack.prod.config.js --progress --colors",
    "start": "npm run build:prod && node ./server/app.js",
    "dev": "webpack-dev-server --content-base src/ --hot --progress --colors --host 0.0.0.0 --port 8080"
  },
  "author": "GenesisSam",
  "license": "ISC",
  "dependencies": {
    "@types/immutable": "^3.8.7",
    "@types/material-ui": "^0.16.51",
    "@types/node": "^7.0.5",
    "@types/react": "^15.0.9",
    "@types/react-dom": "^0.14.23",
    "@types/react-redux": "^4.4.36",
    "@types/react-router": "^3.0.3",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^2.6.34",
    "@types/redux-thunk": "^2.1.0",
    "immutable": "^3.8.1",
    "material-ui": "^0.17.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0",
    "ts-node": "^2.0.0",
    "typescript": "^2.1.6",
    "webfontloader": "^1.6.27",
    "awesome-typescript-loader": "^3.0.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "autoprefixer": "^6.7.5",
    "css-loader": "^0.26.1",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.1",
    "precss": "^1.4.0",
    "react-tap-event-plugin": "^2.0.1",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.2",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.13.1",
    "tslint": "^4.2.0",
